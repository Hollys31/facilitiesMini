/**app.wxss**/

page, view, text, navigator, input, form, select, option, image ,cover-view,cover-image{
  box-sizing: border-box;
}

page {
  background: #f9f9f9;
  font-size: 26rpx;
}

image {
  display: block;
  width: 100%;
}
.mainColor{
  color: #00badb;
}
.placeholder {
  color: #ccc;
  font-size: 24rpx;
}

.icon {
  font-size: 28rpx;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.clearfix {
  zoom: 1;
}

.clearfix:after {
  width: 0;
  height: 0;
  display: block;
  content: "";
  clear: both;
  visibility: none;
}
